<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="burgov_key_value_form.key_value_type" class="Burgov\Bundle\KeyValueFormBundle\Form\Type\KeyValueType">
            <tag name="form.type" alias="burgov_key_value"/>
        </service>
        <service id="burgov_key_value_form.key_value_row_type" class="Burgov\Bundle\KeyValueFormBundle\Form\Type\KeyValueRowType">
            <tag name="form.type" alias="burgov_key_value_row"/>
        </service>
    </services>

</container>
